    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "chalk": "^4.1.2",
    "eslint": "7.32.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^12.0.2",
    "prettier": "^2.4.1",
    "rollup": "^2.60.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {},
  "engines": {
    "node": ">=12.x"
